{
  "manifest_version": 2,

  "name": "Hidden Bookmarks",
  "description": "-- Will be injected from package.json --",
  "version": "-- Will be injected from package.json --",

  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },

  "permissions": [
    "bookmarks"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' http://localhost:%PORT%; object-src 'self'"
}
